# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'basic_app' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for basic_app
  # Redes y Autenticación
  pod 'Alamofire', '~> 5.10.2'
  pod 'AppAuth' 

  # Seguridad y Protección
  # Seguridad y Protección
  pod 'FirebaseAppCheck'


  # Firebase
  pod 'Firebase/Core'
  pod 'Firebase/Analytics'
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'

  # Google Utilities
  pod 'GoogleUtilities'
  pod 'GoogleDataTransport'

  # Interoperabilidad y Protocol Buffers
  pod 'GTMSessionFetcher'
  pod 'leveldb-library'
  pod 'nanopb'
  pod 'PromisesObjC'
  pod 'SwiftProtobuf'


  target 'basic_appTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'basic_appUITests' do
    # Pods for testing
  end

end
